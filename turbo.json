{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "lint": {
      "outputs": []
    },
    "typecheck": {
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "build": {
      "outputs": [
        "dist/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "build:watch": {
      "outputs": [
        "dist/**"
      ],
      "cache": false,
      "dependsOn": [
        "@aokiapp/reark-renderer#build",
        "@aokiapp/reark-lark-api#build",
        "@aokiapp/reark-server#build",
        "@aokiapp/reark#build"
      ],
      "persistent": true
    },
    "dev": {
      "outputs": [
        "dist/**"
      ],
      "cache": false,
      "dependsOn": [
        "^build"
      ],
      "persistent": true
    }
  },
  "ui": "tui"
}