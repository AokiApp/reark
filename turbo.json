{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "lint": {
      "outputs": []
    },
    "typecheck": {
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "build": {
      "outputs": [
        "dist/**"
      ]
    },
    "build:watch": {
      "outputs": [
        "dist/**"
      ],
      "cache": false,
      "dependsOn": [
        "^build"
      ]
    },
    "dev": {
      "outputs": [
        "dist/**"
      ],
      "cache": false,
      "dependsOn": [
        "^build"
      ]
    }
  }
}