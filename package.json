{
  "name": "reark",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "lint": "turbo run lint",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
