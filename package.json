{
  "name": "@aokiapp/reark",
  "private": false,
  "version": "0.1.0",
  "files": [
    "dist/",
    "renderer/dist/",
    "lark-api/dist/",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "lint": "eslint .",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "typecheck": "tsc -p lark-api/tsconfig.json --noEmit && tsc -p renderer/tsconfig.json --noEmit",
    "build": "tsc -p lark-api/tsconfig.json && cd renderer && vite build"
  },
  "devDependencies": {
    "@types/mime-types": "^2.1.4",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "turbo": "^1.13.0",
    "typescript": "~5.6.2",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^4.5.3"
  },
  "exports": {
    "./renderer": {
      "types": "./renderer/dist/index.d.ts",
      "default": "./renderer/dist/index.js"
    },
    "./renderer/style.css": "./renderer/dist/style.css",
    "./lark-api": {
      "types": "./lark-api/dist/index.d.ts",
      "default": "./lark-api/dist/index.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "dependencies": {
    "mime-types": "^3.0.1"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}