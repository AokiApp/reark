{
  "name": "@aokiapp/reark-renderer",
  "version": "0.1.1-pre.0",
  "description": "Lark to React renderer core for Aokiapp monorepo",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "author": "",
  "license": "MIT",
  "private": false,
  "exports": {
