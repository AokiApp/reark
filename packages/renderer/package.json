  },
  "peerDependencies": {
    "react": ">=18.0.0, >=19.0.0",
    "react-dom": ">=18.0.0, >=19.0.0"
  },
  "scripts": {
    "build": "vite build",
    "build:watch": "vite build --watch",
    "dev": "npm run build:watch"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "typescript": "~5.6.2",
    "vite-plugin-dts": "^4.5.3"
  },
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/"
